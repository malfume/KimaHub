local library = loadstring(game:HttpGet('https://raw.githubusercontent.com/cypherdh/VanisUILIB/main/.gitignore'))()

local Window = library:CreateWindow("Child Molester Hub", "Version", 10044538000)

local Tab = Window:CreateTab("Scripts")


local Page = Tab:CreateFrame("Opend Source")


Button = Page:CreateButton("Universal Hub", "admin cmd", function()
    loadstring(game:HttpGetAsync("https://raw.githubusercontent.com/richie0866/orca/master/public/snapshot.lua"))()
CreateNotification("Title", "Unpatached", function(value)
if value == true then
print(true)
else
print(false)
end
end)
end)


