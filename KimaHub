local Rayfield = loadstring(game:HttpGet('https://raw.githubusercontent.com/shlexware/Rayfield/main/source'))()

local Window = Rayfield:CreateWindow({
   Name = "KuroHub",
   LoadingTitle = "KindoHub",
   LoadingSubtitle = "by Zane",
   ConfigurationSaving = {
      Enabled = true,
      FolderName = nil, -- Create a custom folder for your hub/game
      FileName = "Kuro Hub"
   },
   Discord = {
      Enabled = false,
      Invite = "ABCD", -- The Discord invite code, do not include discord.gg/
      RememberJoins = true -- Set this to false to make them join the discord every time they load it up
   },
   KeySystem = true, -- Set this to true to use our key system
   KeySettings = {
      Title = "kuro Hub",
      Subtitle = "Key System",
      Note = "Join the discord (discord.gg/Ek7u7g37)",
      FileName = "KuroKey",
      SaveKey = true,
      GrabKeyFromSite = false, -- If this is true, set Key below to the RAW site you would like Rayfield to get the key from
      Key = "haki" -- The key to gain access
   }
})


local Tab = Window:CreateTab("Opend source", 4483362458) -- Title, Image


local Button = Tab:CreateButton({
   Name = "orca",
   Callback = function()
      loadstring(game:HttpGetAsync("https://raw.githubusercontent.com/richie0866/orca/master/public/snapshot.lua"))()
   end,
})


